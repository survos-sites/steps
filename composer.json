{
    "type": "project",
    "name": "survos-sites/steps",
    "description": "Slideshows created from castor files    ",
    "license": "proprietary",
    "minimum-stability": "beta",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^3.10.3",
        "doctrine/doctrine-bundle": "^2.18.1",
        "doctrine/doctrine-migrations-bundle": "^3.6",
        "doctrine/orm": "^3.5.3",
        "easycorp/easyadmin-bundle": "^4.27.1",
        "jolicode/castor": "dev-main",
        "league/csv": "^9.27.1",
        "meilisearch/search-bundle": "^0.15.9",
        "phpdocumentor/reflection-docblock": "^5.6.3",
        "phpstan/phpdoc-parser": "^2.3",
        "sensiolabs/ansi-to-html": "^2.0",
        "survos/barcode-bundle": "^2.0.45",
        "survos/core-bundle": "^2.0.45",
        "survos/import-bundle": "^2.0.45",
        "survos/installer": "^2.1.4",
        "survos/jsonl-bundle": "dev-main",
        "survos/step-bundle": "dev-main",
        "symfony/asset": "^7.3|^8.0",
        "symfony/asset-mapper": "^7.3.5|^8.0.4",
        "symfony/console": "^7.3.6|^8.0.4",
        "symfony/doctrine-messenger": "^7.3.6|^8.0.4",
        "symfony/dotenv": "^7.3.2|^8.0.2",
        "symfony/expression-language": "^7.3.2|^8.0.2",
        "symfony/flex": "^2.9.0",
        "symfony/form": "^7.3.6|^8.0.4",
        "symfony/framework-bundle": "^7.3.6|^8.0.4",
        "symfony/http-client": "^7.3.6|^8.0.4",
        "symfony/intl": "^7.3.5|^8.0.4",
        "symfony/mailer": "^7.3.5|^8.0.4",
        "symfony/mime": "^7.3.4|^8.0.4",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "^7.3.3|^8.0.3",
        "symfony/process": "^7.3.4|^8.0.4",
        "symfony/property-access": "^7.3.3|^8.0.3",
        "symfony/property-info": "^7.3.5|^8.0.4",
        "symfony/runtime": "^7.3.4|^8.0.4",
        "symfony/security-bundle": "^7.3.4|^8.0.4",
        "symfony/serializer": "^7.3.5|^8.0.4",
        "symfony/stimulus-bundle": "^2.31",
        "symfony/string": "^7.3.4|^8.0.4",
        "symfony/translation": "^7.3.4|^8.0.4",
        "symfony/twig-bundle": "^7.3.4|^8.0.4",
        "symfony/ux-turbo": "^2.31",
        "symfony/validator": "^7.3.6|^8.0.4",
        "symfony/web-link": "^7.3|^8.0",
        "symfony/yaml": "^7.3.5|^8.0.3",
        "twig/extra-bundle": "^2.12|^3.22",
        "twig/twig": "^2.12|^3.22.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "survos/installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR% --symlink --relative": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.3",
            "docker": true
        }
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.4.1",
        "doctrine/doctrine-fixtures-bundle": "^4.3",
        "phpunit/phpunit": "^12.4.2",
        "survos/deployment-bundle": "^2.0.45",
        "symfony/browser-kit": "^7.3.6|^8.0.2",
        "symfony/css-selector": "^7.3.6|^8.0",
        "symfony/debug-bundle": "^7.3.5|^8.0.4",
        "symfony/maker-bundle": "^1.64",
        "symfony/panther": "^2.2",
        "symfony/stopwatch": "^7.3|^8.0",
        "symfony/web-profiler-bundle": "^7.3.5|^8.0.4"
    }
}
